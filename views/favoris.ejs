<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1><%= title %></h1>
<form action='/'>
    <button type="submit">Accueil</button>
</form>

<table>
    <% results.forEach(function(pokemon){ %>
        <form method="POST" action='/removeFavorit/<%= pokemon.id %>'>
            <tr>
                <td><%= pokemon.name %></td>
                <td><button type="submit">Supprimer des favoris</button></td>
            </tr>
        </form>
    <% }); %>
</table>
</body>
</html>